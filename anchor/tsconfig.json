{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "ES2015",
    "types": ["jest", "node"],
    "typeRoots": [
      "../node_modules/@types",
      "./node_modules/@types"
    ],
    "lib": ["es2015"],
    "target": "es6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}